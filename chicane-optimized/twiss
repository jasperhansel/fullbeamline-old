place r13 alpha
place r23 beta
place r33 norm_emittance
set curve alpha.g.a data_source = beam
set curve alpha.g.b data_source = beam
set curve beta.g.a data_source = beam
set curve beta.g.b data_source = beam
set plot alpha autoscale_x = T
set plot alpha autoscale_y = T
set plot beta autoscale_x = T
set plot beta autoscale_y = T
end_file
